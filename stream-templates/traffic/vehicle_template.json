    {
        "@id": "vehicle:$VehicleID$",
        "@type": "sosa:Platform",
        "its:vehModel": "$Type$",
        "sosa:hosts":[
         {
          "@id": "vehicle:SensorMove_$VehicleID$",
          "@type": "sosa:Sensor",
          "sosa:madeObservation":[
           {
                "@id"   : "obs:Move_$ObsID$",
                "@type" : "sosa:Observation",
                "sosa:observedProperty":{
                    "@id":"obsProp:Move"
                },
                "sosa:hasResult":{
                    "its:speed": $Speed$,
                    "its:heading": $Orient_Heading$,
                    "its:acceleration": $Accel$
                },
                "sosa:resultTime": "$Timestamp$"
            }
           ]
          },
          {
            "@id": "vehicle:SensorGPS_$VehicleID$",
            "@type": "sosa:Sensor",
            "sosa:madeObservation":[
             {
                "@id" : "obs:GPS_$ObsID$",
                "@type" : "sosa:Observation",
                "sosa:observedProperty":{
                    "@id":"obsProp:Location"
                },
                "sosa:hasResult":{
                    "@id" : "result:GPS_$ObsID$",
                    "schema:latitude": $Position_X$,
                    "schema:longitude": $Position_Y$
                    "its:onLaneID": $LaneID$,
                    "its:onLaneOrientation": $LaneOrient$
                },
                "sosa:resultTime": "$Timestamp$"
            }
           ]
          }
         ]
    }
